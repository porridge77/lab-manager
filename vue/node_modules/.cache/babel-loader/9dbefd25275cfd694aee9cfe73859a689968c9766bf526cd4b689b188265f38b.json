{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Manager\",\n  currentSemesterName: '',\n  data() {\n    return {\n      currentSemesterName: '',\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}')\n    };\n  },\n  created() {\n    if (!this.user.id) {\n      this.$router.push('/login');\n    }\n  },\n  mounted() {\n    this.getCurrentSemester(); // 在组件加载完成后调用方法\n  },\n\n  methods: {\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}'); // 重新获取下用户的最新信息\n    },\n\n    goToPerson() {\n      if (this.user.role === 'ADMIN') {\n        this.$router.push('/adminPerson');\n      }\n    },\n    getCurrentSemester() {\n      this.$request.get('/semester/current').then(res => {\n        this.currentSemesterName = res.data?.currentSemesterName;\n      });\n    },\n    logout() {\n      localStorage.removeItem('xm-user');\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["name","currentSemesterName","data","user","JSON","parse","localStorage","getItem","created","id","$router","push","mounted","getCurrentSemester","methods","updateUser","goToPerson","role","$request","get","then","res","logout","removeItem"],"sources":["src/views/Manager.vue"],"sourcesContent":["<template>\r\n  <div class=\"manager-container\">\r\n    <!--  头部  -->\r\n    <div class=\"manager-header\">\r\n      <div class=\"manager-header-left\">\r\n        <i class=\"el-icon-s-platform\" style=\"font-size: 30px; font-weight: 1000\"></i>\r\n        <div class=\"title\">实验室管理平台</div>\r\n      </div>\r\n\r\n      <div class=\"manager-header-center\">\r\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n          <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n          <el-breadcrumb-item :to=\"{ path: $route.path }\">{{ $route.meta.name }}</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n        <div style=\"margin-left: 200px; color: #8c939d\">\r\n          <h3>当前学期:{{currentSemesterName}}</h3>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"manager-header-right\">\r\n        <el-dropdown placement=\"bottom\">\r\n          <div class=\"avatar\">\r\n            <img :src=\"'https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png'\" />\r\n            <div>{{ user.name ||  '管理员' }}</div>\r\n          </div>\r\n          <el-dropdown-menu slot=\"dropdown\">\r\n            <el-dropdown-item @click.native=\"logout\">退出登录</el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </el-dropdown>\r\n      </div>\r\n    </div>\r\n\r\n    <!--  主体  -->\r\n    <div class=\"manager-main\">\r\n      <!--  侧边栏  -->\r\n      <div class=\"manager-main-left\">\r\n        <el-menu :default-openeds=\"['info', 'user']\" router style=\"border: none\" :default-active=\"$route.path\">\r\n          <el-menu-item index=\"/home\">\r\n            <i class=\"el-icon-s-home\"></i>\r\n            <span slot=\"title\">系统首页</span>\r\n          </el-menu-item>\r\n          <el-submenu index=\"info\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>信息管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/semester\">学期信息</el-menu-item>\r\n            <el-menu-item index=\"/lab\">实验室信息</el-menu-item>\r\n          </el-submenu>\r\n\r\n          <el-submenu index=\"user\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>用户管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/admin\">管理员信息</el-menu-item>\r\n            <el-menu-item index=\"/labadmin\">实验室管理员</el-menu-item>\r\n            <el-menu-item index=\"/teacher\">教师</el-menu-item>\r\n            <el-menu-item index=\"/student\">学生</el-menu-item>\r\n          </el-submenu>\r\n        </el-menu>\r\n      </div>\r\n\r\n      <!--  数据表格  -->\r\n      <div class=\"manager-main-right\">\r\n        <router-view @update:user=\"updateUser\" />\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Manager\",\r\n  currentSemesterName:'',\r\n  data() {\r\n    return {\r\n      currentSemesterName:'',\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n    }\r\n  },\r\n  created() {\r\n    if (!this.user.id) {\r\n      this.$router.push('/login')\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getCurrentSemester(); // 在组件加载完成后调用方法\r\n  },\r\n  methods: {\r\n    updateUser() {\r\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}')   // 重新获取下用户的最新信息\r\n    },\r\n    goToPerson() {\r\n      if (this.user.role === 'ADMIN') {\r\n        this.$router.push('/adminPerson')\r\n      }\r\n    },\r\n    getCurrentSemester() {\r\n      this.$request.get('/semester/current')\r\n          .then(res => {\r\n            this.currentSemesterName = res.data?.currentSemesterName\r\n          })\r\n    },\r\n    logout() {\r\n      localStorage.removeItem('xm-user')\r\n      this.$router.push('/login')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import \"@/assets/css/manager.css\";\r\n</style>"],"mappings":";AAuEA;EACAA,IAAA;EACAC,mBAAA;EACAC,KAAA;IACA;MACAD,mBAAA;MACAE,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,UAAAL,IAAA,CAAAM,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,kBAAA;EACA;;EACAC,OAAA;IACAC,WAAA;MACA,KAAAZ,IAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;;IACAS,WAAA;MACA,SAAAb,IAAA,CAAAc,IAAA;QACA,KAAAP,OAAA,CAAAC,IAAA;MACA;IACA;IACAE,mBAAA;MACA,KAAAK,QAAA,CAAAC,GAAA,sBACAC,IAAA,CAAAC,GAAA;QACA,KAAApB,mBAAA,GAAAoB,GAAA,CAAAnB,IAAA,EAAAD,mBAAA;MACA;IACA;IACAqB,OAAA;MACAhB,YAAA,CAAAiB,UAAA;MACA,KAAAb,OAAA,CAAAC,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}