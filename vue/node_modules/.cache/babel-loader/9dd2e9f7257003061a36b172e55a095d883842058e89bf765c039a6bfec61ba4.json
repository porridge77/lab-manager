{"ast":null,"code":"export default {\n  name: \"StuReservation\",\n  data() {\n    return {\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      status: null,\n      fromVisible: false,\n      form: {},\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      rules: {},\n      ids: [],\n      labData: []\n    };\n  },\n  created() {\n    this.getCurrentSemester();\n    this.load(1);\n    this.loadLab();\n  },\n  methods: {\n    loadLab() {\n      this.$request.get('/lab/selectAll').then(res => {\n        if (res.code === '200') {\n          this.labData = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleAdd() {\n      // 新增数据\n      this.form = {}; // 新增数据的时候清空数据\n      this.fromVisible = true; // 打开弹窗\n    },\n\n    getCurrentSemester() {\n      this.$request.get('/semester/current').then(res => {\n        this.form.semesterName = res.data;\n      });\n    },\n    changeStatus(row, status) {\n      let data = JSON.parse(JSON.stringify(row));\n      if ('通过' === status) {\n        data.status = status;\n      }\n      if ('驳回' === status) {\n        data.status = status;\n      }\n      if ('使用完毕' === status) {\n        data.status = '使用完毕';\n      }\n      this.$request.put('/stureservation/update', data).then(res => {\n        if (res.code === '200') {\n          this.$message.success('操作成功');\n          this.load(1);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    save() {\n      // 保存按钮触发的逻辑  它会触发新增或者更新\n      this.form.studentId = this.user.id;\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.$request({\n            url: this.form.id ? '/stureservation/update' : '/stureservation/add',\n            method: this.form.id ? 'PUT' : 'POST',\n            data: this.form\n          }).then(res => {\n            if (res.code === '200') {\n              // 表示成功保存\n              this.$message.success('保存成功');\n              this.load(1);\n              this.fromVisible = false;\n            } else {\n              this.$message.error(res.msg); // 弹出错误的信息\n            }\n          });\n        }\n      });\n    },\n\n    del(id) {\n      // 单个删除\n      this.$confirm('您确定取消预约吗？', '灵魂拷问', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete('/stureservation/delete/' + id).then(res => {\n          if (res.code === '200') {\n            // 表示操作成功\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    },\n    load(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/stureservation/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          status: this.status\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    reset() {\n      this.status = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","status","fromVisible","form","user","JSON","parse","localStorage","getItem","rules","ids","labData","created","getCurrentSemester","load","loadLab","methods","$request","get","then","res","code","$message","error","msg","handleAdd","semesterName","changeStatus","row","stringify","put","success","save","studentId","id","$refs","formRef","validate","valid","url","method","del","$confirm","type","response","delete","catch","params","list","reset","handleCurrentChange"],"sources":["src/views/manager/StuReserve.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"search\">\r\n      <el-select v-model=\"status\" placeholder=\"请选择审核状态\" style=\"width: 200px\">\r\n        <el-option label=\"待审核\" value=\"待审核\"></el-option>\r\n        <el-option label=\"审核通过\" value=\"审核通过\"></el-option>\r\n        <el-option label=\"审核不通过\" value=\"审核不通过\"></el-option>\r\n      </el-select>\r\n      <el-button type=\"info\" plain style=\"margin-left: 10px\" @click=\"load(1)\">查询</el-button>\r\n      <el-button type=\"warning\" plain style=\"margin-left: 10px\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div class=\"operation\" v-if=\"user.role === 'STUDENT'\">\r\n      <el-button type=\"primary\" plain @click=\"handleAdd\">新增</el-button>\r\n    </div>\r\n\r\n    <div class=\"table\">\r\n      <el-table :data=\"tableData\" stripe>\r\n        <el-table-column prop=\"id\" label=\"序号\" width=\"80\" align=\"center\" sortable></el-table-column>\r\n        <el-table-column prop=\"labNo\" label=\"实验室\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"semesterName\" label=\"学期\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"week\" label=\"周数\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"section\" label=\"节次\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"reason\" label=\"原因\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"date\" label=\"申请日期\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"studentName\" label=\"预约人\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"status\" label=\"预约状态\"></el-table-column>\r\n\r\n        <el-table-column label=\"操作\" width=\"180\" align=\"center\">\r\n          <template v-slot=\"scope\">\r\n            <el-button plain type=\"warning\" size=\"mini\" @click=\"del(scope.row.id)\" v-if=\"user.role === 'STUDENT' && scope.row.status === '未审核'\">取消预约</el-button>\r\n            <el-button plain type=\"warning\" size=\"mini\" @click=\"changeStatus(scope.row, '通过')\" v-if=\"user.role !== 'STUDENT' && scope.row.status === '未审核'\">通过</el-button>\r\n            <el-button plain type=\"warning\" size=\"mini\" @click=\"changeStatus(scope.row, '驳回')\" v-if=\"user.role !== 'STUDENT' && scope.row.status === '未审核'\">驳回</el-button>\r\n            <el-button plain type=\"warning\" size=\"mini\" @click=\"changeStatus(scope.row, '使用完毕')\" v-if=\"scope.row.status === '通过'\">结束使用</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n\r\n    <el-dialog title=\"预约\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <el-form label-width=\"100px\" style=\"padding-right: 50px\" :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n        <el-form-item prop=\"labId\" label=\"实验室编号\">\r\n          <el-select v-model=\"form.labId\" placeholder=\"请选择实验室\" style=\"width: 100%\">\r\n            <el-option v-for=\"item in labData\" :label=\"item.no\" :value=\"item.id\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item prop=\"week\" label=\"周次\">\r\n          <el-input v-model=\"form.week\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"section\" label=\"节次\">\r\n          <el-input v-model=\"form.section\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"reason\" label=\"原因\">\r\n          <el-input v-model=\"form.reason\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"date\" label=\"日期\">\r\n          <el-date-picker v-model=\"form.date\" type=\"date\" placeholder=\"选择日期\" format=\"yyyy 年 MM 月 dd 日\" value-format=\"yyyy-MM-dd\"></el-date-picker>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"StuReservation\",\r\n  data() {\r\n    return {\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      status: null,\r\n      fromVisible: false,\r\n      form: {},\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      rules: {\r\n\r\n      },\r\n      ids: [],\r\n      labData: [],\r\n    }\r\n  },\r\n  created() {\r\n    this.getCurrentSemester()\r\n    this.load(1)\r\n    this.loadLab()\r\n  },\r\n  methods: {\r\n    loadLab(){\r\n      this.$request.get('/lab/selectAll').then(res => {\r\n        if (res.code === '200') {\r\n          this.labData = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleAdd() {   // 新增数据\r\n      this.form = {}  // 新增数据的时候清空数据\r\n      this.fromVisible = true   // 打开弹窗\r\n    },\r\n    getCurrentSemester() {\r\n      this.$request.get('/semester/current')\r\n          .then(res => {\r\n            this.form.semesterName = res.data\r\n          })\r\n    },\r\n    changeStatus(row, status) {\r\n      let data = JSON.parse(JSON.stringify(row))\r\n      if ('通过' === status) {\r\n        data.status = status\r\n      }\r\n      if ('驳回' === status) {\r\n        data.status = status\r\n      }\r\n      if ('使用完毕' === status) {\r\n        data.status = '使用完毕'\r\n      }\r\n      this.$request.put('/stureservation/update', data).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success('操作成功')\r\n          this.load(1)\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    save() {   // 保存按钮触发的逻辑  它会触发新增或者更新\r\n      this.form.studentId = this.user.id\r\n      this.$refs.formRef.validate((valid) => {\r\n        if (valid) {\r\n          this.$request({\r\n            url: this.form.id ? '/stureservation/update' : '/stureservation/add',\r\n            method: this.form.id ? 'PUT' : 'POST',\r\n            data: this.form\r\n          }).then(res => {\r\n            if (res.code === '200') {  // 表示成功保存\r\n              this.$message.success('保存成功')\r\n              this.load(1)\r\n              this.fromVisible = false\r\n            } else {\r\n              this.$message.error(res.msg)  // 弹出错误的信息\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    del(id) {   // 单个删除\r\n      this.$confirm('您确定取消预约吗？', '灵魂拷问', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/stureservation/delete/' + id).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    load(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/stureservation/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          status: this.status,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    reset() {\r\n      this.status = null\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAkFA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,MAAA;MACAC,WAAA;MACAC,IAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,KAAA,GAEA;MACAC,GAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,kBAAA;IACA,KAAAC,IAAA;IACA,KAAAC,OAAA;EACA;EACAC,OAAA;IACAD,QAAA;MACA,KAAAE,QAAA,CAAAC,GAAA,mBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAV,OAAA,GAAAS,GAAA,CAAAxB,IAAA;QACA;UACA,KAAA0B,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAC,UAAA;MAAA;MACA,KAAAtB,IAAA;MACA,KAAAD,WAAA;IACA;;IACAW,mBAAA;MACA,KAAAI,QAAA,CAAAC,GAAA,sBACAC,IAAA,CAAAC,GAAA;QACA,KAAAjB,IAAA,CAAAuB,YAAA,GAAAN,GAAA,CAAAxB,IAAA;MACA;IACA;IACA+B,aAAAC,GAAA,EAAA3B,MAAA;MACA,IAAAL,IAAA,GAAAS,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAwB,SAAA,CAAAD,GAAA;MACA,aAAA3B,MAAA;QACAL,IAAA,CAAAK,MAAA,GAAAA,MAAA;MACA;MACA,aAAAA,MAAA;QACAL,IAAA,CAAAK,MAAA,GAAAA,MAAA;MACA;MACA,eAAAA,MAAA;QACAL,IAAA,CAAAK,MAAA;MACA;MACA,KAAAgB,QAAA,CAAAa,GAAA,2BAAAlC,IAAA,EAAAuB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAS,OAAA;UACA,KAAAjB,IAAA;QACA;UACA,KAAAQ,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAQ,KAAA;MAAA;MACA,KAAA7B,IAAA,CAAA8B,SAAA,QAAA7B,IAAA,CAAA8B,EAAA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAArB,QAAA;YACAsB,GAAA,OAAApC,IAAA,CAAA+B,EAAA;YACAM,MAAA,OAAArC,IAAA,CAAA+B,EAAA;YACAtC,IAAA,OAAAO;UACA,GAAAgB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cAAA;cACA,KAAAC,QAAA,CAAAS,OAAA;cACA,KAAAjB,IAAA;cACA,KAAAZ,WAAA;YACA;cACA,KAAAoB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;YACA;UACA;QACA;MACA;IACA;;IACAiB,IAAAP,EAAA;MAAA;MACA,KAAAQ,QAAA;QAAAC,IAAA;MAAA,GAAAxB,IAAA,CAAAyB,QAAA;QACA,KAAA3B,QAAA,CAAA4B,MAAA,6BAAAX,EAAA,EAAAf,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YAAA;YACA,KAAAC,QAAA,CAAAS,OAAA;YACA,KAAAjB,IAAA;UACA;YACA,KAAAQ,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;UACA;QACA;MACA,GAAAsB,KAAA,QACA;IACA;IACAhC,KAAAhB,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAmB,QAAA,CAAAC,GAAA;QA